<Window x:Class="RecibosDeCaja.ViewDocuments"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RecibosDeCaja"
        xmlns:SiaSf="http://schemas.syncfusion.com/wpf"             
        mc:Ignorable="d"
        Title="ViewDocuments" Height="450" Width="800" Loaded="Window_Loaded">

    <SiaSf:TabControlExt Name="TabControl1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"  SiaSf:SkinStorage.VisualStyle="Metro"  IsDisableUnloadTabItemExtContent="False" EnableLabelEdit="False" TabItemSelectedBackground="Orange"/>

</Window>
